  gem 'less-rails'
  gem 'therubyracer'
  gem 'coffee-rails'
  gem 'uglifier'
end

group :development, :test do
  gem 'pry-rails'
  gem 'rspec-rails'

  # Guard
  gem 'guard-rspec'

  # Notification
  gem 'growl'
end
